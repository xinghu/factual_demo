<div class="col-xs-20 col-sm-10 col-md-10 col-lg-10" style="margin:20px">
	<div class="row">
		<%= form_tag("/home", {:class => 'form-inline', :role => "form"}) do |f| %>
  <div class="form-group">
    <input type="text" class="form-control" id="name" name="name" placeholder="Name" value="<%= @name %>">
  </div>
  <div class="form-group">
    <input type="text" class="form-control" id="region" name="region" placeholder="Region" value="<%= @region %>">
  </div>
  
  <button type="submit" class="btn btn-default">Search</button>
<% end %>
</div>
</div>


<div class="col-xs-20 col-sm-10 col-md-10 col-lg-10">
	<h2>Results:</h2>
<% @rows.each do |row| %>
 <p><%= row  %></p>
<% end %>
</div>

